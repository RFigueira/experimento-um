<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.codepampa.experimento_um">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NotificationUm"
            android:label="@string/title_experimento1"/>
        <activity android:name=".NotificationDois"
            android:label="@string/title_experimento2"/>
        <activity android:name=".NotificationTres"
            android:label="@string/title_experimento3"/>
        <activity android:name=".NotificationQuatro"
            android:label="@string/title_experimento4"/>
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_atividade2">
        </activity>
    </application>

</manifest>